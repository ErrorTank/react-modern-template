.create-block-route{
  .trans{
    .submit-btn{
      margin-top 15px
      font-size 13px
    }
  }
  .create-form{
    color #1a1a1a
    font-size 15px

    .submit-btn{
      margin-top 20px !important
      color white
      font-size 16px
      font-weight 550
      padding 8px

    }
    .form-section{
      margin-bottom 15px !important
      .static-label{
        color tikiblue
        font-weight 550
      }
      .static-value{
        word-wrap break-word
      }
    }
  }
  .empty-notify{
    color #1a1a1a
    font-size 15px
  }
  .wrapper{
    display flex
    justify-content space-between
    .border-block{
      border 2px solid #1a1a1a
      background-color white
      .cart-item{
        font-size 13px
        padding 10px
        .label{
          color tikiblue
          font-weight 550
        }
        .value{
          color #1a1a1a
          word-wrap break-word
        }
      }
      border-radius 3px
      width 35%
      &:last-child{
        margin-left 20px
        width 65%
      }

    }
  }

}