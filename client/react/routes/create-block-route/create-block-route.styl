.create-block-route{
  .empty-notify{
    color #1a1a1a
    font-size 13px
  }
  .wrapper{
    display flex
    justify-content space-between
    .border-block{
      border 2px solid #1a1a1a
      .cart-item{
        font-size 13px
        padding 10px
        .label{
          color tikiblue
          font-weight 550
        }
        .value{
          color #1a1a1a
          word-wrap break-word
        }
      }
      border-radius 3px
      width 35%
      &:last-child{
        margin-left 20px
        width 65%
      }

    }
  }

}