.view-chain-route{
  .main-info{
    margin-bottom 30px
    z-index -1
    font-size 18px
    .label{
      color tikiblue
      font-weight 550
    }
    .value{
      color #1a1a1a
      max-width 300px
      word-wrap break-word
    }
  }
  .empty-chain{
    text-align center
    color #1a1a1a
    font-size 18px
  }
  .main-section{
    margin auto
    margin-top 50px
    width 600px
  }
  .special-block{
    color danger
  }
  .block{
    .trans-title{
      color #1a1a1a
      font-weight 550
      margin 10px 0
    }
    .trans{
      padding-left 15px
    }
    padding 50px
    margin auto
    margin-bottom 20px
    border 1px solid #1a1a1a
    border-radius 3px
    .info-block{
      display flex
      justify-content space-between
      font-size 16px
      .label{
        color tikiblue
        font-weight 550
      }
      .value{
        color #1a1a1a
        max-width 300px
        word-wrap break-word
      }
    }
  }
}